{
  "name": "aurora-test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "^12.18.4",
    "yarn": "^1.22.5"
  },
  "scripts": {
    "start": "node index.js",
    "start:dev": "nodemon index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "mocha"
  },
  "dependencies": {
    "uuid": "8.3.2",
    "validator": "13.5.2",
    "zod": "1.11.11"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/eslint-parser": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "chai": "4.2.0",
    "eslint": "7.19.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "mocha": "8.2.1",
    "nodemon": "2.0.7",
    "sinon": "9.2.4"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaVersion": 12
    },
    "extends": [
      "airbnb-base"
    ],
    "rules": {
      "class-methods-use-this": "off",
      "no-underscore-dangle": "off",
      "no-use-before-define": "off",
      "no-restricted-syntax": "off"
    }
  }
}
